{
  "name": "swagger-panel",
  "displayName": "Swagger Panel",
  "description": "swagger api doc panel",
  "publisher": "elsaooo",
  "version": "1.0.0",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ElsaOOo/vscode-swagger-panel.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:swagger-panel.helloWorld",
    "onCommand:swagger-panel.swaggerDoc",
    "onView:swagger-doc"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "swagger-panel.swaggerDoc",
        "title": "swagger doc"
      },
      {
        "command": "swagger-doc.addSwagger",
        "title": "Add"
      },
      {
        "command": "swagger-doc.deleteSwagger",
        "title": "Delete"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "swagger-doc",
          "name": "Swagger Doc"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "swagger-doc",
        "contents": "No swagger url found.\n[Add Swagger Url](command:swagger-doc.addSwagger)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "swagger-doc.addSwagger",
          "when": "view == swagger-doc"
        }
      ],
      "view/item/context": [
        {
          "command": "swagger-doc.deleteSwagger",
          "when": "view == swagger-doc && viewItem == swaggerItem",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "ts-loader": "^9.2.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  }
}
